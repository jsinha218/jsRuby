##if comparison
##	statements
##elsif	comparison
##	statements
##elsif comparison
##	statements
##else
##	statements
##end
	
grade = gets
grade = Integer(grade)
if grade >= 90
	puts("Grade A")
elsif grade > 80 && grade < 90
	puts("Grade B")
elsif grade > 70 && grade <= 80
	puts("Grade C")
else
	puts("FAIL")
end
